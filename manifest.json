{
  "manifest_version": 2,
  "name": "Image Search Extension",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.3.min.js", "jquery-ui.min.js", "searcher.js"],
      "css": ["jquery-ui.css"]
    }
  ],
  "icons": {
    "18": "18.png",
    "48": "48.png"
  }
}